<?xml version="1.0" encoding="utf-8"?>
<layout>
    <android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/content_equipo__inof"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.example.y2793623b.teams_players_info.Equipo_InofFragment"
        tools:showIn="@layout/activity_equipo__inof">

        <ImageView
            android:layout_width="0dp"
            android:layout_height="193dp"
            app:srcCompat="@mipmap/ic_launcher"
            android:visibility="visible"
            android:id="@+id/Foto"
            android:adjustViewBounds="true"
            app:layout_constraintRight_toLeftOf="@+id/guideline5"
            android:layout_marginEnd="8dp"
            android:layout_marginStart="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/guideline6"
            android:layout_marginBottom="8dp"
            android:contentDescription="" />

        <TextView
            android:text="@string/name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/txtName"
            app:layout_constraintTop_toTopOf="@+id/guideline6"
            android:layout_marginTop="8dp"
            android:layout_marginStart="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/guideline5"
            android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.2"
            app:layout_constraintBottom_toTopOf="@+id/guideline8"
            android:layout_marginBottom="8dp" />

        <TextView
            android:text="@string/code"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/txtCode"
            android:layout_marginStart="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/guideline5"
            android:layout_marginEnd="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline8"
            android:layout_marginTop="8dp"
            app:layout_constraintHorizontal_bias="0.54"
            app:layout_constraintBottom_toTopOf="@+id/guideline9"
            android:layout_marginBottom="8dp" />

        <TextView
            android:text="@string/shortname"
            android:layout_width="0dp"
            android:layout_height="33dp"
            android:id="@+id/txtShortName"
            android:layout_marginStart="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/guideline5"
            android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="16dp"
            app:layout_constraintTop_toTopOf="@+id/guideline10"
            android:layout_marginTop="8dp"
            app:layout_constraintHorizontal_bias="0.54"
            app:layout_constraintVertical_bias="0.04000002" />

        <TextView
            android:text="@string/squadmarketvalue"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/txtMarketValue"
            android:layout_marginStart="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/guideline5"
            android:layout_marginEnd="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline9"
            android:layout_marginTop="8dp"
            app:layout_constraintHorizontal_bias="0.7"
            app:layout_constraintBottom_toTopOf="@+id/guideline10"
            android:layout_marginBottom="8dp" />

        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline5"
            app:layout_constraintGuide_begin="273dp"
            android:orientation="vertical"
            tools:layout_editor_absoluteY="0dp"
            tools:layout_editor_absoluteX="273dp" />

        <android.support.constraint.Guideline
            android:layout_width="384dp"
            android:layout_height="wrap_content"
            android:id="@+id/guideline6"
            app:layout_constraintGuide_begin="233dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteY="233dp"
            tools:layout_editor_absoluteX="0dp" />

        <android.support.constraint.Guideline
            android:layout_width="384dp"
            android:layout_height="wrap_content"
            android:id="@+id/guideline8"
            app:layout_constraintGuide_begin="279dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteY="279dp"
            tools:layout_editor_absoluteX="0dp" />

        <android.support.constraint.Guideline
            android:layout_width="384dp"
            android:layout_height="wrap_content"
            android:id="@+id/guideline9"
            app:layout_constraintGuide_begin="319dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteY="319dp"
            tools:layout_editor_absoluteX="0dp" />

        <android.support.constraint.Guideline
            android:layout_width="384dp"
            android:layout_height="0dp"
            android:id="@+id/guideline10"
            app:layout_constraintGuide_begin="367dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteY="367dp"
            tools:layout_editor_absoluteX="0dp" />

    </android.support.constraint.ConstraintLayout>


</layout>



